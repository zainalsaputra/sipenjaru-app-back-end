{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 0,
        "no-useless-constructor": 0,
        "import/no-dynamic-require": 0,
        "no-shadow" : 0,
        "global-require" : 0,
        "class-methods-use-this" : 0,
        "consistent-return": 0
        
    }
}
